<h1>New</h1>
<div class="row">
  <div class="span6 offset3">
    <%= form_for(@computer) do |f| %>

      <%= f.label :asset_number %>
      <%= f.text_field :asset_number %>

      <%= f.label :sn %>
      <%= f.text_field :sn %>

      <%= f.label :ip %>
      <%= f.text_field :sn %>

      <%= f.label :idrac_ip %>
      <%= f.text_field :idrac_ip %>

      <%= f.label :machine_cabinet %>
      <%= collection_select(:computer, :machine_cabinet_id, MachineCabinet.all, :id, :number) %>

      <%= f.label :Project %>
      <%= select_tag :project_id, options_for_select(Project.all.collect {|project| [project.name, project.id]}, @project_list),:multiple => true, :prompt => 'Select Project' %>
      <%= f.submit "Create new item", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
